	public int ackermann(int m, int n) {
		int ret;
      	if (m < 1) 
      		ret = n + 1;
      	else if (n < 1) 
      		ret = this.ackermann(m - 1, 1);
      	else
      		ret = this.ackermann(m - 1, this.ackermann(m, n - 1));
      		return ret;
	}
	
	
